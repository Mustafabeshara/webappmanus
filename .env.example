# Database (MySQL - Required)
# Railway provides this automatically when you add a MySQL service
DATABASE_URL=mysql://user:password@host:3306/database

# Authentication (Required)
VITE_APP_ID=your_manus_app_id
JWT_SECRET=generate_a_secure_random_string_at_least_32_chars
OAUTH_SERVER_URL=https://oauth.manus.app
OWNER_OPEN_ID=your_owner_open_id

# Server Configuration
PORT=3000
NODE_ENV=production

# Optional - Forge API for OCR features
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=

# Optional - Analytics (Umami)
VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=

# Optional - AWS S3 for file storage
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
AWS_S3_BUCKET=
